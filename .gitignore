# Ignore bundler config
/.bundle

# Ignore fetched packages.
/node_modules
/bower_components

# Ignore the build directory
/build
/dist

# Ignore cache
/.sass-cache
/.cache

# Ignore log files.
*.log
*.log.*

# Ignore .DS_store file
.DS_Store

# Ignore ctags files
.tags*
